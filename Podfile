platform :ios, '11.0'

use_frameworks!

target "MuchToDo" do

    pod 'TableKit', '~> 2.6' # declarative table views
    pod 'SnapKit', '~> 4.0' # auto layout
    pod 'Reveal-SDK', " ~> 16", :configurations => ['Debug'] # debugging views
    pod 'PinLayout'
    pod 'RealmSwift'
    pod 'SwiftyChrono' # natural language date parsing

end
